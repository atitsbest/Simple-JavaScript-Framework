<div align="center">
	{+new Link().toFile("index.html").withText("Classes")+} |
	{+new Link().toClass("_global_").withText("Globals")+} |
	{+new Link().toFile("files.html").withText("Files")+}
</div>
<hr />
<h2>Classes</h2>
<form action="#">
	<div class="patternInput">
		<input type="text" name="classPattern" id="classPattern" value="" />
	</div>
</form>
<ul class="classList" id="classList">
	<for each="thisClass" in="data">
	{!
		if (thisClass.alias != "_global_") {
			output += "<li>" + new Link().toClass(thisClass.alias) + "</li>";
		}
	!}
	</for>
</ul>
<hr />
<h2>Globals</h2>
<form action="#">
	<div class="patternInput">
		<input type="text" name="globalsPattern" id="globalsPattern" value="" />
	</div>
</form>
<ul class="classList" id="globalsList">
	<for each="thisClass" in="data">
	{!
		if (thisClass.alias == "_global_") {
			var ownMethods = thisClass.methods.filter(function($){return $.memberOf == thisClass.alias}).sort(makeSortby("name"));
			!}
				<for each="member" in="ownMethods">
				{!
					var fileName = FilePath.fileName(member.srcFile);
					var memberAlias = member.alias.replace(thisClass.alias, "");
				!}
					<li>{+new Link().toFile("symbols/" + thisClass.alias + fileName + publish.conf.ext + "#" + memberAlias).withText(member.name)+}</li>
				</for>
			{!
		}
	!}
	</for>
</ul>
<hr />